<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.bwpl.TestPL" version="0.1." xmlns="http://apache.org/cordova/ns/plugins/1.0"
        xmlns:android="http://schemas.android.com/apk/res/android">
    <name>TestPL</name>
    <js-module name="TestPL" src="www/TestPL.js">
        <clobbers target="cordova.plugins.TestPL"/>
    </js-module>
    <platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="TestPL">
                <param name="ios-package" value="TestPL"/>
            </feature>
        </config-file>
        <source-file src="src/ios/TestPL.m"/>
    </platform>
</plugin>